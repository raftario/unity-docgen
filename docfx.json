{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.dll"
          ]
        }
      ],
      "dest": "obj"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "index.md",
          "obj/**.yml",
          "toc.yml"
        ]
      }
    ],
    "dest": "docs",
    "template": [
      "default"
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "disableGitFeatures": true,
    "globalMetadata": {
      "_appTitle": "API Documentation",
      "_appFooter": "API Documentation",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableContribution": true
    }
  }
}
