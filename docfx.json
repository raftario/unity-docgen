{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.dll"
          ]
        }
      ],
      "dest": "obj"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "index.md",
          "obj/**.yml",
          "toc.yml"
        ]
      }
    ],
    "dest": "docs",
    "template": [
      "statictoc"
    ],
    "globalMetadata": {
      "_disableContribution": true
    }
  }
}
